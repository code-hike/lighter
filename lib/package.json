{
  "name": "@code-hike/lighter",
  "type": "module",
  "version": "1.0.1",
  "description": "Code Hike's syntax highlighter",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "worker": "./dist/worker.esm.mjs",
      "browser": "./dist/browser.esm.mjs",
      "default": "./dist/index.esm.mjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./*": "./dist/*"
  },
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/pluginutils": "^5.0.2",
    "@types/react": "^18.0.26",
    "jsonc-parser": "^3.2.0",
    "node-fetch": "^3.3.1",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vitest": "1.0.0-beta.1",
    "vscode-oniguruma": "1.7.0",
    "vscode-textmate": "9.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "vitest --test-timeout 10000",
    "typecheck": "vitest typecheck",
    "regenerate": "node ./utils/update.mjs"
  },
  "repository": "https://github.com/code-hike/lighter",
  "homepage": "https://codehike.org",
  "funding": "https://github.com/sponsors/code-hike",
  "dependencies": {
    "ansi-sequence-parser": "1.1.1",
    "tm-grammars": "^1.21.10"
  }
}
